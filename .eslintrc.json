{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": true }],
        "consistent-return": "off",
        "no-promise-executor-return": "off"
    }
}